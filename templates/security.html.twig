<!DOCTYPE html>
<html lang="fr">
<!-- begin::Head -->
<head>
    <meta charset="utf-8" />
    <title>{% block title %}{{ 'Compte connexion ' ~ compagny }}{% endblock %}</title>
    <meta name="description" content="Latest updates and statistic charts">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: {"families":["Poppins:300,400,500,600,700","Roboto:300,400,500,600,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!--end::Web font -->
    <!--begin::Base Styles -->
    <link href="{{ asset('build/css/vendors.bundle.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('build/css/style.bundle.css') }}" rel="stylesheet" type="text/css" />
    {% block stylesheets %}{% endblock %}
    <!--end::Base Styles -->
    <link rel="shortcut icon" href="{{ asset('assets/apps/layout/media/img/logo/favicon.ico') }}" />
</head>
<!-- end::Head -->
<!-- end::Body -->
<body class="m--skin- m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default">
    <!-- begin:: Page -->
    {% block body %}{% endblock %}
    <!-- end:: Page -->
    <!--begin::Base Scripts -->
    <script src="{{ asset('assets/vendors/base/vendors.bundle.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/apps/layout/base/scripts.bundle.js') }}" type="text/javascript"></script>
    <!--end::Base Scripts -->
    <!--begin::Page Snippets -->
    <script src="{{ asset('build/js/login.bundle.js') }}" type="text/javascript"></script>
    <!--end::Page Snippets -->
    {% block javascripts %}{% endblock %}
</body>
</html>